        "by_date":{
            "description": "clock and date that the activity or product should be done by",
            "type" : "datetime"
            
                }
            }
        ]
    },
    "required": ["list_name", "prod_or_act_name", "importance" ],
    "optional": ["sidenote", "quantity","by_date"]
}

